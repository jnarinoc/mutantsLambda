{
  "name": "mutants",
  "version": "1.0.0",
  "description": "get mutants ",
  "author": "",
  "private": true,
  "main": "build/bin/www.js",
  "scripts": {
    "build": "tsc",
    "start": "npm run build && export DEBUG=*:* && node ./build/bin/www.js",
    "dev": "nodemon --watch src -e ts,js --exec npm start",
    "test:file": " mocha --timeout 10000 --require ts-node/register",
    "test:file:example": "npm run test:file -- 'test/example.spec.ts' -g 'Test Class Name'",
    "test:only": "npm run test:file -- 'test/**/*.spec.ts'",
    "test": " npm run test:only",
    "coverage": "export $(cat test/.env-test | xargs) && nyc _mocha --timeout 10000 --require ts-node/register 'test/**/*.spec.ts'",
    "coverage:report": "nyc report --reporter=lcov --reporter=text",
    "coverage:web": "npm run coverage && http-server coverage/",
    "sonar": "gulp sonar",
    "lint": "tslint -c tslint.json -p tsconfig.json",
    "lint:fix": "npm run lint -- --fix",
    "debug": "DEBUG=* node ./build/bin/www.js",
    "performance": "./performance/run-clinic.sh",
    "clean": "rm -rf .nyc_output coverage build .scannerwork performance/*.clinic-doctor*"
  },
  "license": "ISC",
  "dependencies": {
    "aws-serverless-express": "~3.3.6",
    "axios": "^0.21.0",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "express-actuator": "~1.1.0",
    "express-openapi-validate": "^0.5.0",
    "faker": "^5.1.0",
    "http-server": "^0.12.3",
    "mocha": "^6.1.4",
    "morgan": "~1.9.1",
    "pg": "^8.2.1",
    "node-fetch": "^2.6.0",
    "nodemon": "^2.0.6",
    "nyc": "^15.1.0",
    "qs": "^6.9.4",
    "secure-random-string": "^1.1.2",
    "sequelize": "^6.3.5",
    "ts-node": "^9.0.0",
    "tsc": "^1.20150623.0",
    "tslint": "^6.1.3",
    "typescript": "^4.1.2"
  },
  "devDependencies": {
    "@types/cookie-parser": "~1.4.1",
    "@types/dateformat": "^3.0.1",
    "@types/debug": "^4.1.5",
    "@types/express": "^4.17.9",
    "@types/express-actuator": "^1.3.0",
    "@types/faker": "^5.1.5",
    "@types/mocha": "~5.2.7",
    "@types/morgan": "~1.7.36",
    "@types/nock": "~10.0.3",
    "@types/node": "^12.12.41",
    "@types/node-fetch": "^2.5.5",
    "@types/sinon": "~7.0.13",
    "@types/secure-random-string": "^1.1.0",
    "@types/validator": "^13.1.2",
    "chai": "~4.2.0",
    "chai-http": "~4.3.0",
    "nock": "^13.0.5",
    "sequelize-cli": "^6.2.0",
    "sinon": "^9.2.1",
    "ts-node": "~8.2.0",
    "tslint": "~5.17.0",
    "typescript": "~3.5.1"
  }
}
